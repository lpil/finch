<h2>Bundles</h2>

<ul>
<%= for bundle <- @bundles do %>
  <li>
    <div>
      <label>Display Name</label>:
      <%= bundle.display_name %>
    </div>
    <div>
      <label>Code</label>:
      <code><%= bundle.code %></code>
    </div>
  </li>
<% end %>
</ul>

<p><%= link "New Bundle", to: bundle_path(@conn, :new) %></p>
<p><%= link "Back", to: "/" %></p>
